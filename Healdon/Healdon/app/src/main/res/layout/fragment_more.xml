<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MoreFragment">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="@drawable/top_pane"
            android:gravity="center"
            android:text="More Settings"
            android:textAlignment="gravity"
            android:textColor="@color/itemcolortransparent"
            android:textSize="30sp" />

        <LinearLayout
            android:id="@+id/constraintLayout6"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:background="@drawable/menu_bg"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="MoCA Test"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="218dp"
                android:background="@drawable/rightarrow" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout8"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/menu_bg">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="Set Water Drinking Reminder"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />
            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="60dp"
                android:src="@drawable/rightarrow" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout9"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/menu_bg">

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="Medication Reminder"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />
            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="125dp"
                android:src="@drawable/rightarrow" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout10"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/menu_bg">

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_gravity="center_vertical"
                android:text="Set Routine"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />
            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="212dp"
                android:src="@drawable/rightarrow"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout11"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/menu_bg">

            <TextView
                android:id="@+id/signout_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_gravity="center_vertical"
                android:text="Sign Out"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="240.5dp"
                android:src="@drawable/rightarrow" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout12"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/menu_bg">

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_gravity="center_vertical"
                android:text="Reminder Settings"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp"/>

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="151dp"
                android:src="@drawable/rightarrow"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout13"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/menu_bg">

            <TextView
                android:id="@+id/nearbydoc_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Nearby Doctors"
                android:onClick="go_to_nearby_doctors"
                android:layout_gravity="center_vertical"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="176dp"
                android:src="@drawable/rightarrow" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/constraintLayout14"
            android:layout_width="391dp"
            android:layout_height="66dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/bmenu_bg">

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:text="Appointment Status"
                android:textColor="@color/itemcolortransparent"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="138dp"
                android:src="@drawable/rightarrow" />

        </LinearLayout>

    </LinearLayout>

</FrameLayout>